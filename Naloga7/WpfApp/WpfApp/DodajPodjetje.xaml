<Window x:Class="WpfApp.DodajPodjetje"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="DodajPodjetje" Height="300" Width="300">
    <Grid>
        <Label Content="Ime:" HorizontalAlignment="Left" Margin="46,59,0,0" VerticalAlignment="Top"/>
        <Label Content="Panoga:" HorizontalAlignment="Left" Margin="46,99,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtBIme" HorizontalAlignment="Left" Height="23" Margin="114,63,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtBPanoga" HorizontalAlignment="Left" Height="23" Margin="114,103,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="bttPotrdi" Content="Potrdi" HorizontalAlignment="Left" Margin="46,173,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.139,0.254" Click="bttPotrdi_Click"/>
        <Button x:Name="bttPreklici" Content="Preklici" HorizontalAlignment="Left" Margin="159,173,0,0" VerticalAlignment="Top" Width="75" Click="bttPreklici_Click"/>

    </Grid>
</Window>
